{
	"parser": "babel-eslint",
	"extends": [
		"airbnb-base"
	],
	"rules": {
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": true
			}
		],
		"operator-linebreak": [
			"error",
			"after"
		],
		"comma-dangle": 0,
		"linebreak-style": 0
	},
	"env": {
		"browser": true
	}
}